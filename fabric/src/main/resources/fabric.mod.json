{
  "schemaVersion": 1,
  "id": "pooped",
  "version": "${version}",
  "name": "Pooped",
  "description": "Decorating your home just got better! Another Furniture adds various furniture, with the goal of staying vanilla-styled and consistently sized, keeping normal blocks and player sizes in mind.",
  "authors": [
    "Starfish Studios"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/another-furniture",
    "issues": "https://github.com/starfish-studios/AnotherFurniture/issues"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.starfish_studios.pooped.fabric.PoopedFabric"
    ],
    "client": [
      "com.starfish_studios.pooped.fabric.PoopedFabricClient"
    ]
  },
  "mixins": [
    "pooped.mixins.json",
    "pooped-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": ">=1.19.2"
  }
}
